class Solution(object):
    def wordBreak(self, s, wordDict):
        """
        :type s: str
        :type wordDict: Set[str]
        :rtype: bool
        """
        if not s or not wordDict:
             return False

        n = len(s)
        dp = [False] * (n+1)
        dp[0] = True
        
        for i in range(n):
            for j in range(i, -1, -1):
                if not dp[j]:
                    continue
                if ''.join(s[j:i+1]) in wordDict:
                    dp[i+1] = True
                    break
        
        return dp[-1]
